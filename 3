#include <stdio.h>

int main()
{
	int K, N; //Задаем день
	printf_s("K: "); //Вывод надписи
	scanf_s("%d", &K); //Ввод информации
	printf_s("N: "); //Вывод надписи
	scanf_s("%d", &N); //Ввод информации
	while (K > 7) //До тех пор, пока значение больше 7, вычитаем по одной неделе.
		K -= 7;
	K = K + 2; //Также учитываем, что год начался с 3 дня недели.
	printf_s("%i", K); //Вывод
	return 0; //Выход
}
