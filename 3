#include <stdio.h>

int main()
{
    int K = 17; //Задаем день
    int N = 7; //Задаем первый день недели в году
    while (K > 7) //До тех пор, пока значение больше 7, вычитаем по одной неделе.
      K -= 7;
    K = K + 2; //Также учитываем, что год начался с 3 дня недели.
    printf_s("%i", K); //Вывод
    return 0; //Выход
}
