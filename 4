#include <stdio.h>

int main() 
{
	int A, B, C; //Задаем стороны прямоугольника и квадрата.
	printf_s("A: "); //Вывод надписи
	scanf_s("%d", &A); //Ввод информации
	printf_s("B: "); //Вывод надписи
	scanf_s("%d", &B); //Ввод информации
	printf_s("C: "); //Вывод надписи
	scanf_s("%d", &C); //Ввод информации
	int N = ((A / C) * (B / C)); //Рассчитываем кол-во квадратов в прямоугольнике.
	int S = (A * B - N * C * C); //Рассчитываем оставшуюся площадь.
	//Выводим результаты
	printf_s("%i\n", N);
	printf_s("%i", S);
	return 0; //Выход
}
