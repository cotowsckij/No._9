#include <stdio.h>

int main() {
    //Задаем стороны прямоугольника и квадрата.
    int A = 5; 
    int B = 9;
    int C = 4;
    int N = ((A / C) * (B / C)); //Рассчитываем кол-во квадратов в прямоугольнике.
    int S = (A * B - N * C * C); //Рассчитываем оставшуюся площадь.
    //Выводим результаты
    printf_s("%i\n", N); 
    printf_s("%i", S);
    return 0; //Выход
}
